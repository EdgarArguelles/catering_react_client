{
  "name": "catering_react_client",
  "version": "1.0.0",
  "description": "This project is a React Client used by CATERING",
  "scripts": {
    "webpack-dev": "cross-env API_URL=https://cateringjavaapi.herokuapp.com webpack-dev-server -d --history-api-fallback --port 3001 --host 0.0.0.0",
    "webpack-prod": "cross-env API_URL=https://cateringjavaapi.herokuapp.com webpack -p",
    "dev": "npm run eslint && npm run webpack-dev",
    "prod": "npm run build && node server",
    "build": "npm run eslint && npm run test && npm run webpack-prod",
    "eslint": "eslint --max-warnings=-0 --ext .js,jsx ./",
    "test": "cross-env API_URL=https://testhost/catering jest",
    "start": "node server",
    "heroku-postbuild": "npm run webpack-prod"
  },
  "eslintIgnore": [
    "dist/**",
    "coverage/**"
  ],
